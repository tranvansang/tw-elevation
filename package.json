{
	"name": "tw-elevation",
	"version": "0.1.1",
	"description": "tailwindcss elevation plugin",
	"main": "index.js",
	"repository": "https://github.com/tranvansang/tw-elevation.git",
	"files": [
		"index.js",
		"index.d.ts",
		"index.js.map",
		"config.js",
		"config.d.ts",
		"config.js.map",
		"util.js",
		"util.d.ts",
		"util.js.map"
	],
	"scripts": {
		"build": "rm -rf index.d.ts index.js.map index.js config.js config.js.map config.d.ts util.js util.d.ts util.js.map && tsc --downlevelIteration --declaration --sourceMap --esModuleInterop --outDir . --lib esnext index.ts --skipLibCheck"
	},
	"author": "Tran Sang <t@sang.jp>",
	"license": "MIT",
	"devDependencies": {
		"@types/color-name": "^1.1.1",
		"@types/node": "^16.11.12",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.3"
	},
	"dependencies": {
		"color-name": "^1.1.4"
	}
}
