{
	"name": "tw-elevation",
	"version": "0.2.1",
	"description": "tailwindcss elevation plugin",
	"main": "index.js",
	"repository": "https://github.com/tranvansang/tw-elevation.git",
	"files": [
		"index.js",
		"index.d.ts",
		"config.js",
		"config.d.ts",
		"util.js",
		"util.d.ts",
		"cjs"
	],
	"exports": {
		".": {
			"import": "./index.js",
			"require": "./cjs/index.js"
		}
	},
	"type": "module",
	"scripts": {
		"prepublishOnly": "rm -rf index.d.ts index.js config.js config.d.ts util.js util.d.ts && tsc --target esnext --module commonjs --downlevelIteration --declaration --inlineSourceMap --esModuleInterop --outDir . --lib esnext index.ts --skipLibCheck"
	},
