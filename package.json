	"devDependencies": {
		"@types/color-name": "^1.1.1",
		"@types/node": "^16.11.12",
		"ts-node": "^10.4.0",
		"typescript": "^4.5.3"
	},
	"dependencies": {
		"color-name": "^1.1.4"
	}
}
